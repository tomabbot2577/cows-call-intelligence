# RingCentral API
RINGCENTRAL_CLIENT_ID=REDACTED_CLIENT_ID
RINGCENTRAL_CLIENT_SECRET=REDACTED_CLIENT_SECRET
RINGCENTRAL_JWT_TOKEN=REDACTED_JWT_TOKEN
RINGCENTRAL_SERVER_URL=https://platform.ringcentral.com

Add the following content:
```env
# Database
DATABASE_URL=postgresql://call_system:REDACTED_PASSWORD@localhost:5432/call_recordings



# Google Drive
GOOGLE_CREDENTIALS_PATH=/var/www/call_recording_system/config/google_service_account.json
GOOGLE_DRIVE_FOLDER_ID=https://drive.google.com/drive/u/2/folders/1IbGtmzk85Q5gYAfdb2AwA9kLNE1EJLx0


# Whisper Settings
WHISPER_MODEL=base
WHISPER_DEVICE=cpu
WHISPER_COMPUTE_TYPE=int8

# Processing Settings
BATCH_SIZE=50
MAX_WORKERS=4
DAILY_SCHEDULE_TIME=02:00
HISTORICAL_DAYS=60
MAX_RETRIES=3

# Monitoring
PROMETHEUS_ENABLED=true
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=REDACTED_EMAIL
SMTP_PASSWORD=REDACTED_PASSWORD
ALERT_EMAIL=alerts@mainsequence.net
SLACK_WEBHOOK=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Storage Paths
AUDIO_STORAGE_PATH=/var/www/call_recording_system/data/audio
TRANSCRIPT_STORAGE_PATH=/var/www/call_recording_system/data/transcripts
LOG_FILE_PATH=/var/www/call_recording_system/logs/app.log
```
